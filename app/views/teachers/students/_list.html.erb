<h1 class="mt-3 mb-5">
  <span class="highlight-orange"><%= @course.division.name %> - List of Students</span>
</h1>


<div class="row">
  <div class="col-6">
    <div class="ml-3">
      <% @new_course_students.first(5).each do |student| %>

        <!-- CARD -->
        <div class="frontside mb-3">
            <div class="card-student-list" data-email="<%= student.email %>"data-name="<%=  student.first_name%>" data-score="<%=student.grade%>" data-photo="<%=student.picture_url%>" data-label="<%=@course.material.chapter_ids%>" data-value="<%=student.flashcards_score_student(student.division.courses.first.material).to_json%>">
              <div class="card-body-student d-flex">
                <div class="photo-nom">
                  <%= image_tag student.picture_url, alt: "card image", class: "img-fluid #{student.border_color(@material)}" %>
                  <h20 class="card-title">
                    <%= student.first_name%>
                  </h20>
                </div>
                <p class="card-student-text flex-grow-1"><%= student.cluster_message_student(@material,@course) %></p>
                <p><%= student.grade %>/100</p>

                <div class="eye-icon">
                  <img class="student-details-icon"src="data:image/svg+xml;utf8;base64," />
                </div>
              </div>
            </div>
        </div>

      <% end %>


      <!-- COLLAPSE BUTTON -->
      <p>
        <button class="btn btn-sherpa" type="button-list" data-toggle="collapse" data-target=".multi-collapse" aria-expanded="false" aria-controls="multiCollapseExample1 multiCollapseExample2">See Full List</button>
      </p>

      <div class="collapse multi-collapse" id="multiCollapseExample1">
        <% @list_students_without_top_offender.each do |student| %>

          <!-- CARD -->
          <div class="frontside mb-3">
            <div class="card-student-list" data-email="<%= student.email %>"data-name="<%=  student.first_name%>" data-score="<%=student.grade%>" data-photo="<%=student.picture_url%>" data-label="<%=@course.material.chapter_ids%>" data-value="<%=student.flashcards_score_student(student.division.courses.first.material).to_json%>">
              <div class="card-student-list">
                <div class="card-body-student d-flex">
                  <div class="photo-nom">
                    <%= image_tag student.picture_url, alt: "card image", class: "img-fluid #{student.border_color(@material)}" %>
                    <h20 class="card-title">
                      <%= student.first_name%>
                    </h20>
                  </div>
                  <p class="card-student-text flex-grow-1"><%=  student.cluster_message_student(@material,@course) %></p>
                  <p><%= student.grade %>/100</p>
                  <div class="eye-icon">
                    <img class="student-details-icon"src="data:image/svg+xml;utf8;base64," />
                  </div>
                </div>
              </div>
            </div>
          </div>

        <% end %>
      </div>
    </div>
  </div>


  <!-- STUDENT SHOW  -->
  <div class="col-6">
    <div class="sticky-top">

    <%= render 'teachers/students/individual_show'%>
    </div>
  </div>

</div>

      <!-- ./Team member -->
